<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- namespace：命名空间，用于隔离sql，实际使用中，与mapper绑定 -->
<mapper namespace="com.dianshang.dao.front.TwoDao">
    <!-- sql语句 -->
    <select id="gettwos" parameterType="One" resultType="Two">
        select * from two where pid=#{id}
    </select>
</mapper>